                    </Button>
                </WrapPanel>
                <WrapPanel Margin="0 10 0 0">
                    <ProgressBar Name="Analyzeprogress" Height="23" Width="425" Visibility="Hidden"/>
                    <Button Content="Open" Name="OpenButton" Click="OpenButton_Click" Width="70" Margin="10 0 0 0"/>
                </WrapPanel>
            </StackPanel>
        <DataGrid Visibility="Visible" AutoGenerateColumns="False" Grid.Row="1" RowHeaderWidth="0" ItemsSource="{Binding}" DataContext="{StaticResource ResultData}" Margin="20 0 20 20">
            <DataGrid.Columns>
                <DataGridTextColumn Header="SrlNo" Binding="{Binding SrlNo}"/>
                <DataGridTextColumn Header="ConnsignmentNo" Binding="{Binding ConnNo}"/>
                <DataGridTextColumn Header="Destination" Binding="{Binding Destination}"/>
                <DataGridTextColumn Header="Service" Binding="{Binding serviceCode}"/>
                <DataGridTextColumn Header="Amount" Binding="{Binding Amount}"/>
                <DataGridTextColumn Header="hasError" Binding="{Binding hasError}"></DataGridTextColumn>
                <DataGridTextColumn Header="Error Discription" Binding="{Binding MisMatchDesc}"/>
            </DataGrid.Columns>
        </DataGrid>
        <Canvas Height="26" Width="1024" VerticalAlignment="Top">
            <Path Data="F1M1,1C1,1 497,1 497,1 497,1 497,25 497,25 497,25 1,25 1,25 1,25 1,1 1,1z" Fill="#FFCBCBCB" Height="26" Canvas.Left="0" Canvas.Top="0" Width="1024" Stretch="Fill" Stroke="Black" />
            <TextBlock Name="AddUpdateTitle" FontSize="16" Height="25" IsHyphenationEnabled="True" LineStackingStrategy="BlockLineHeight" Canvas.Left="0" LineHeight="18" TextAlignment="Center" TextWrapping="Wrap" Text="Invoice Report" Canvas.Top="6" Width="1024" FontFamily="Arial"/>
            <Path MouseDown="DragthisWindow" x:Name="Memu_bar" Data="F1M1,1C1,1 497,1 497,1 497,1 497,25 497,25 497,25 1,25 1,25 1,25 1,1 1,1z" Fill="Transparent" Height="26" Canvas.Left="0" Canvas.Top="0" Width="1024" Stretch="Fill" />
            <Button ToolTip="Close" Height="18" Canvas.Right="4" Click="Button_Click_Close" Canvas.Top="4" Width="35" Style="{StaticResource 1}">
                <Path x:Name="close" Data="F1M14,12.8C14,12.8 12.8,14 12.8,14 12.8,14 8,9.2 8,9.2 8,9.2 3.2,14 3.2,14 3.2,14 2,12.8 2,
