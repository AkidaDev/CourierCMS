<Window x:Class="FinalUi.InvoiceReport"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="Invoice Report" Width="1074" Height="650" Background="Transparent"
		ResizeMode="NoResize" WindowStyle="None" WindowStartupLocation="CenterScreen" WindowState="Normal" AllowsTransparency="True" Padding="20">
    <Window.Resources>
        <CollectionViewSource x:Key="ResultData" CollectionViewType="ListCollectionView"></CollectionViewSource>
    </Window.Resources>
    <Grid Height="600" UseLayoutRounding="False"  Width="1024" Background="White">
        <Grid.Effect>
            <DropShadowEffect BlurRadius="10" Direction="314" RenderingBias="Quality" ShadowDepth="0"/>
        </Grid.Effect>
            <Grid.RowDefinitions>
                <RowDefinition Height="115"/>
                <RowDefinition Height="*"/>
            </Grid.RowDefinitions>
            <StackPanel Grid.Row="0" HorizontalAlignment="Center" Margin="0 50 0 0">
                <WrapPanel HorizontalAlignment="Center">
                    <TextBlock Text="Open File .: " FontSize="16" FontWeight="Bold" Foreground="#FF5B5B5B" FontFamily="Calibri" Padding="0 2 0 0"/>
                    <Rectangle Stroke="DarkGray" Width="300" Height="22"/>
                    <TextBlock Name="FilePathBlock" Padding="3 2 0 0" Width="300" Height="22" Margin="-300 0 0 0"/>
                    <Button ToolTip="Browse" Name="BrowseButton" Click="BrowseButton_Click" Style="{DynamicResource 1}" Margin="10 0 0 0">
