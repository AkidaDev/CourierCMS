<Window x:Class="FinalUi.Login"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
          xmlns:gif="http://wpfanimatedgif.codeplex.com"
		Icon="Images/logo.png"
        Title="Login" Height="319" Width="640" Background="Transparent"
        WindowStyle="None" WindowStartupLocation="CenterScreen" BorderBrush="Transparent" AllowsTransparency="True" Padding="20">

    <Grid Name="MainGrid" Height="319" Width="623" MouseLeftButtonDown="Grid_MouseLeftButtonDown_1"  KeyDown="MainGrid_KeyUp">
        <Grid.Effect>
            <DropShadowEffect Color="Black" RenderingBias="Quality" Direction="0" ShadowDepth="0" BlurRadius="10"/>
        </Grid.Effect>
        <Grid.Background>
            <ImageBrush ImageSource="Images/vortex.png"/>
        </Grid.Background>
        <StackPanel>
            
        </StackPanel>
        <DockPanel LastChildFill="False" Background="Black"><StackPanel DockPanel.Dock="Left" HorizontalAlignment="Left">
		</StackPanel>
		<StackPanel HorizontalAlignment="Right" DockPanel.Dock="Top" Margin="0,3,3,0" Background="#FFff0000">
		<Button Style="{StaticResource all}" Name="CloseButton" Click="CloseButton_Click" Width="60" Background="Red" Height="20"><Button.Content>                 
				 <Path x:Name="close1" Data="F1M14.987,13.789C14.987,13.789 13.622,15.154 13.622,15.154 13.622,15.154 8.16,9.692 8.16,9.692 8.16,
				9.692 2.699,15.154 2.699,15.154 2.699,15.154 1.333,13.789 1.333,13.789 1.333,13.789 6.795,8.327 6.795,8.327 6.795,8.327 1.333,2.865 1.333,2.865 1.333,
				2.865 2.699,1.5 2.699,1.5 2.699,1.5 8.16,6.962 8.16,6.962 8.16,6.962 13.622,1.5 13.622,1.5 13.622,1.5 14.987,2.865 14.987,2.865 14.987,2.865 9.526,
				8.327 9.526,8.327 9.526,8.327 14.987,13.789 14.987,13.789z" Fill="Black" Margin="0,-2,0,0"/>
		</Button.Content></Button>
			</StackPanel>
			
		<StackPanel DockPanel.Dock="Left" Margin="0,-19,0,0">
			<Image Name="vort" Height="319" Source="Images/vortex0.png"/>
			</StackPanel>
		<StackPanel DockPanel.Dock="Top">
			<Image Height="56" Source="Images/vortex3.png" Stretch="Fill" Width="181" Margin="0,63,5,0"/>
					</StackPanel>
            <StackPanel DockPanel.Dock="Bottom" HorizontalAlignment="Right" Margin="5,0,12,1">
			
                <Separator Visibility="Hidden"></Separator>

                
                <WrapPanel HorizontalAlignment="Center" Margin="-145,0,0,0">
                    <TextBox Text="SQLEXPRESS" x:Name="InstanceName" MinWidth="150" Padding="5 5 5 5" HorizontalAlignment="Center"/>
                </WrapPanel>
                <Separator Visibility="Hidden"></Separator>
                <WrapPanel HorizontalAlignment="Center" Margin="-145,0,0,0">
                    <TextBox MinWidth="150" Padding="5 5 5 5" Name="UserName" ></TextBox>
                </WrapPanel>
                <Separator Visibility="Hidden"></Separator>
                <WrapPanel HorizontalAlignment="Center" Margin="-145,0,0,-20" >
                    <PasswordBox MinWidth="150" Name="Password" Padding="5 5 5 5"></PasswordBox>
                </WrapPanel>
                <Separator Visibility="Hidden"></Separator>
                <!--<WrapPanel  HorizontalAlignment="Right" Margin="5 0 0 10"><Button Content="Setup Data" x:Name="SetupButton" Click="SetupButton_Click_1" Margin="10 0 0 0" Width="107"/>
                </WrapPanel>-->
                <WrapPanel Name="LoadingGifPanel" Visibility="Hidden">
                    <Image gif:ImageBehavior.AnimatedSource="gifs/loading-GIF.gif" Stretch="None" Name="LoadingGif" Margin="78,13,9,4"  ></Image>
                </WrapPanel>

                <Separator Visibility="Hidden"></Separator>
            </StackPanel>
        </DockPanel>

    </Grid>
</Window>
