<Window x:Class="FinalUi.DeleteConnsignment"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
		Icon="Images/logo.ico"
        Title="Delete Consignment" Height="180" Width="370" WindowStartupLocation="CenterScreen" ResizeMode="CanMinimize">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="5*"/>
            <RowDefinition Height="60"/>
            <RowDefinition Height="40"/>
            <RowDefinition Height="3*"/>
        </Grid.RowDefinitions>
        <WrapPanel Grid.Row="1" VerticalAlignment="Center" HorizontalAlignment="Center">
            <Path Margin="0 -2 0 0" Style="{DynamicResource delete}" Fill="Gray"/>
            <TextBlock FontSize="16" FontFamily="Calibri" Text=" Consignment No. .: " VerticalAlignment="Center"/>
            <TextBox Name="Zonecodebox" Width="160" Padding="2" Height="24"/>          
        </WrapPanel>
        <Button Grid.Row="2" Name="AddZoneButton" Click="AddZoneButton_Click" VerticalAlignment="Bottom" HorizontalAlignment="Right" Margin="0 0 25 15">
            <Button.Content>
                <WrapPanel Margin="6 1">
                    <TextBlock FontSize="16" FontFamily="Calibri" Text=" Delete "/>
                </WrapPanel>
            </Button.Content>
        </Button>
    </Grid>
</Window>
