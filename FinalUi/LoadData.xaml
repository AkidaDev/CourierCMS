<Window x:Class="FinalUi.LoadData"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="LoadData" Height="335.821" Width="300">
    <StackPanel>
        <WrapPanel>
            <TextBox TextWrapping="Wrap"  Name="filename" MinWidth="150" />
            <Button Content="Browser" Click="Button_Click"/>
        </WrapPanel>
        <WrapPanel>
            <CheckBox Name="addData" Content="Add data in currecnt sheet" IsChecked="False" Click="addData_Checked"></CheckBox>
        </WrapPanel>
        <Button Content="Start Load" Style="{StaticResource ButtonStyle}" HorizontalAlignment="Left"  VerticalAlignment="Top"  Click="Button_Click_1"/>
       <TextBlock Text="Load From Database"></TextBlock>
        <DatePicker Name="DataToLoadDate"></DatePicker>
        <Button Content="Start Load" Click="Button_Click_2"></Button>
    </StackPanel>
</Window>
