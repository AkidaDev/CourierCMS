<Window x:Class="FinalUi.PowerEntry"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="PowerEntry" Height="300" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="Auto"></RowDefinition>
            <RowDefinition Height="Auto"></RowDefinition>
            <RowDefinition Height="Auto"></RowDefinition>
            <RowDefinition Height="Auto"></RowDefinition>
        </Grid.RowDefinitions>
        <WrapPanel Grid.Row="0">
            <TextBlock Text="Enter Starting Index: "></TextBlock>
            <ComboBox Name="startConnNo" IsEditable="True" ItemsSource="{Binding}"></ComboBox>
        </WrapPanel>
        <WrapPanel Grid.Row="1">
            <TextBlock Text="Enter Ending Index: "></TextBlock>
            <ComboBox Name="endConnNo" IsEditable="True" ItemsSource="{Binding}"></ComboBox>
        </WrapPanel>
        <WrapPanel Grid.Row="2">
            <TextBlock Text="Select Client: "></TextBlock>
            <ComboBox Name="clientCode" IsEditable="True" ItemsSource="{Binding}"></ComboBox>
        </WrapPanel>
        <WrapPanel Grid.Row="3">
            <Button Content="Go" Name="SubmitRecords" Click="SubmitRecords_Click"></Button>
        </WrapPanel>
    </Grid>
</Window>
